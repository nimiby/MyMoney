{
    "name": "angular-phonecat",
    "private": true,
    "dependencies": {
        "angular": "1.2.x",
        "jquery": "1.10.2",
        "bootstrap": "~3.2.0",
        "angular-route": "~1.2.x",
        "angular-resource": "~1.2.x",
        "angular-animate": "~1.2.x",
        "less": "~1.7.5",
        "font-awesome": "~4.2.0",
        "angular-strap": "~2.1.1"
    },
    "exportsOverride": {
        "angular": {
            ".": "*.*"
        },
        "bootstrap": {
            "js": "dist/js/*.js",
            "less": "less/*.less",
            "less/mixins": "less/mixins/*.less"
        },
        "font-awesome": {
            "less": "less/*.less",
            "fonts": "fonts/*.*"
        }
    }
}
